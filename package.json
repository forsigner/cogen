{
  "name": "cogen-root",
  "private": true,
  "version": "0.0.0",
  "description": "A code generate tool",
  "workspaces": {
    "packages": [
      "packages/*",
      "examples/*",
      "website/*"
    ]
  },
  "scripts": {
    "dev": "lerna run --stream dev",
    "build": "lerna run --stream build",
    "test": "lerna run --stream test",
    "publish": "lerna publish --force-publish=*",
    "prepare": "husky install"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/.history/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forsigner/cogen.git"
  },
  "author": "@forsigner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forsigner/cogen/issues"
  },
  "homepage": "https://github.com/forsigner/cogen#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.27.0",
    "eslint-config-tie-app": "0.0.5",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "prettier": "^2.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "27.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
